<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>edu.tamu.oal.nightlyreboot</string>
	<key>ProgramArguments</key>
	<array>
		<string>/Library/Scripts/OAL/nightlyreboot.sh</string>
	</array>
	<key>KeepAlive</key>
	<dict>
		<key>SuccessfulExit</key>
		<false/>
	</dict>
	<key>ThrottleInterval</key>
	<integer>60</integer>
	<key>StartCalendarInterval</key>
	<dict>
		<key>Minute</key>
		<integer>10</integer>
		<key>Hour</key>
		<integer>17</integer>
	</dict>
</dict>
</plist>
